        "eslint-plugin-jest": "^27.2.1",
        "jest": "^29.5.0",
        "prettier": "^2.6.2",
        "rimraf": "^5.0.1",
        "ts-jest": "^29.1.0",
        "ts-node-dev": "^2.0.0",
        "tslint": "^6.1.3",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "dotenv": "^16.0.3",
